---
import Layout from "../layouts/Layout.astro";
import Container from "../components/container.astro";
import Sectionhead from "../components/sectionhead.astro";
import Card from "../components/card.astro";

const TOURBIKEPRODUCTS = [
  {
    name: "A breathtaking road trip through the Alpujarras",
    description: "",
    href: "/cultural-travels/a-breathtaking-road-trip-through-the-alpujarras",
    href_img:
      "/cultural-travels/a-breathtaking-road-trip-through-the-alpujarras/alp-product.webp",
    alt_img: "string",
  },
];
---

<Layout title="E-Bike Products">
  <Container>
    <Sectionhead>
      <Fragment slot="title">Cultural Travels Tours</Fragment>
      <Fragment slot="desc"></Fragment>
    </Sectionhead>
    <main class="mt-24">
      <ul class="grid md:grid-cols-2 lg:grid-cols-3 gap-6 w-full">
        {
          TOURBIKEPRODUCTS.map((tour) => (
            <li class="mb-10 ">
              <Card {...tour} />
            </li>
          ))
        }
      </ul>
    </main>
  </Container>
</Layout>
