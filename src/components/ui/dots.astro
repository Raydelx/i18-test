---
interface Props {
  class?: string;
}
const { class: className } = Astro.props;
---

<div class={`-mt-1 md:mt-0 ${className}`}>
  <span class="inline-block w-44 h-1 rounded-full bg-[#0E46A3]"></span>
  <span class="inline-block w-3 h-1 ml-1 rounded-full bg-[#0E46A3]"></span>
  <span class="inline-block w-1 h-1 ml-1 rounded-full bg-[#0E46A3]"></span>
</div>
